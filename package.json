    },
    "mac": {
      "category": "your.app.category.type",
      "icon": "icons/mac/icon.icns"
    },
    "win": {
      "icon": "icons/win/icon.ico"
    },
    "linux": {
      "icon": "icons/linux"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "electron-store": "^8.2.0",
    "jquery": "^3.7.1"
  }
}
